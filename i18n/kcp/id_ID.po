msgid "On install action, install as a dependence"
msgstr "Pada tindakan pasang, pasang sebagai ketergantungan"

#: kcp.go:325
msgid "Package not found!"
msgstr "Paket tidak ditemukan!"

#: kcp.go:63
msgid "Print this help"
msgstr "Cetak bantuan ini"

#: kcp.go:64
msgid "Print version"
msgstr "Cetak versi"

#: kcp.go:67
msgid "Search packages' motif in KCP and display them"
msgstr "Cari motif paket-paket di KCP dan tampilannya"

#: kcp.go:61
msgid "Tool in command-line for KaOS Community Packages"
msgstr "Alat dalam baris-perintah untuk KaOS Community Packages"

#: kcp.go:51
msgid "Unknown error!"
msgstr "Galat tidak diketahui!"

#: kcp.go:62
msgid "[OPTIONS] [APP]"
msgstr "[OPSI] [APL]"

#: kcp.go:471
msgid "debug mode"
msgstr "mode awakutu"

